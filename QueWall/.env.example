# QueWall Environment Configuration
# Copy this to .env and customize as needed

# Project name
COMPOSE_PROJECT_NAME=quewall

# Service versions
POSTGRES_VERSION=16-alpine
KEYCLOAK_VERSION=23.0
OAUTH2_PROXY_VERSION=latest
TRAEFIK_VERSION=2.11

# Network
NETWORK_NAME=quewall-network

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KC_DB_NAME=keycloak
KC_DB_USER=keycloak

# Postgres Configuration
POSTGRES_DB=keycloak
POSTGRES_USER=keycloak

# Domain Configuration
DOMAIN_KEYCLOAK=keycloak.theddt.local
DOMAIN_AUTH=auth.theddt.local
DOMAIN_APP=app.theddt.local
