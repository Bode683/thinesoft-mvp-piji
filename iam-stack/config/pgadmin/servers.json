      "SSLCrl": "<STORAGE_DIR>/.postgresql/root.crl",
      "SSLCompression": 0,
      "Timeout": 10,
      "UseSSHTunnel": 0,
      "TunnelHost": "",
      "TunnelPort": "",
      "TunnelUsername": "",
      "TunnelAuthentication": 0
    }
  }
}
